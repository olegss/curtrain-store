<p id="notice"><%= notice %></p>

<h1>Listing Anketa</h1>

<table>
  <thead>
    <tr>
      <th>Name store</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anketa.each do |anketum| %>
      <tr>
        <td><%= anketum.name_store %></td>
        <td><%= anketum.email %></td>
        <td><%= link_to 'Show', anketum %></td>
        <td><%= link_to 'Edit', edit_anketum_path(anketum) %></td>
        <td><%= link_to 'Destroy', anketum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anketum', new_anketum_path %>
